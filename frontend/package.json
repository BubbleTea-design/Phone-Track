{
  "name": "phone-tracker-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "leaflet": "^1.9.3",
    "react-leaflet": "^4.2.1",
    "axios": "^1.3.4",
    "@capacitor/core": "^5.0.0",
    "@capacitor/cli": "^5.0.0",
    "@capacitor/android": "^5.0.0"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "cross-env CI=false react-scripts build",
    "start": "node server.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cap:init": "npx cap init \"Phone Tracker\" \"com.phonetracker.app\"",
    "cap:add": "npx cap add android",
    "cap:copy": "npx cap copy",
    "cap:open": "npx cap open android",
    "cap:build": "npm run build && npx cap copy",
    "apk:build": "npm run build && npx cap copy && npx cap open android"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
